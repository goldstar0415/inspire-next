<dt><strong>Action Type:</strong></dt>
<dd><%= print_or_dashes(@action.type.underscore.humanize) %></dd>
<% if @action.type ==  'SwitchChannelAction' %>
  <%= render 'actions/details/switch_channel_action_fields' %>
<% elsif @action.type == 'SendMessageAction' %>
  <%= render 'actions/details/send_message_action_fields' %>
<% end %>
