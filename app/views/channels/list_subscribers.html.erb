<div class="page-header">
  <h1><%=@channel.name %> subscriber list</h1>
</div>

<div class="row pagination-centered">
  <div class="span6" id="unsubscribed_subscribers">
    <%= render partial:'channels/subscriber_list/unsubscribed_subscribers' %>
  </div>
  <div class="span5" id="subscribed_subscribers">
    <%= render partial:'channels/subscriber_list/subscribed_subscribers' %>
  </div>
</div>


<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              channel_path(@channel), :class => 'btn'  %>
</div>
